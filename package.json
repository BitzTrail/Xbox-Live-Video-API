{
    "name": "@xboxreplay/sample-project",
    "description": "Usage examples of \"@xboxreplay/xboxlive-auth\" and \"@xboxreplay/xboxlive-api\" packages",
    "version": "0.0.1",
    "keywords": [
        "xboxreplay",
        "xboxlive",
        "auth"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/XboxReplay/sample-project"
    },
    "license": "MIT",
    "main": "./dist/index.js",
    "author": {
        "name": "Alexis Bize",
        "email": "alexis.bize@gmail.com"
    },
    "scripts": {
        "start": "npm run serve",
        "build": "npm run clean && ./node_modules/.bin/tsc",
        "clean": "rm -rf ./dist",
        "serve": "npm run build && XBL_EMAIL=${XBL_EMAIL} XBL_PASSWORD=${XBL_PASSWORD} node ./dist/index.js"
    },
    "dependencies": {
        "@xboxreplay/xboxlive-auth": "^0.3.0",
        "@xboxreplay/xboxlive-api": "^0.1.3",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.0",
        "@types/node": "^12.0.7",
        "typescript": "^3.5.1",
        "tsc-watch": "^2.2.1"
    }
}
